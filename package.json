  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThomasJaspers/microplode-frontend/issues"
  },
  "dependencies": {
    "amqp": "^0.2.4",
    "elm": "~0.16.0",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "normalize.css": "^3.0.3",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "rimraf": "^2.4.4"
  }
}
